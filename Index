  <script>
    const compressionLabel = document.getElementById("compressionLabel");
    const compressionRange = document.getElementById("compressionRange");
    const downloadBtn = document.getElementById("downloadBtn");

    compressionRange.oninput = () => {
      compressionLabel.textContent = compressionRange.value + "%";
    };

    function compressImageWithAd() {
      const file = document.getElementById("imageInput").files[0];
      const quality = compressionRange.value / 100;

      if (!file) {
        alert("Please select an image.");
        return;
      }

      // ðŸ‘‡ Open Adsttra Direct Link Ad https://denialtransportationfair.com/p23e981d?key=e35ec5cecd79cd0e6b939867910e619e
      window.open("https://www.profitabledisplaynetwork.com/YOUR_ADSTTRA_DIRECT_LINK", "_blank");

      // Wait and compress after ad click
      setTimeout(() => {
        const reader = new FileReader();
        reader.onload = function (e) {
          const img = new Image();
          img.src = e.target.result;

          img.onload = function () {
            const canvas = document.createElement("canvas");
            canvas.width = img.width;
            canvas.height = img.height;

            const ctx = canvas.getContext("2d");
            ctx.drawImage(img, 0, 0);

            const compressedDataUrl = canvas.toDataURL("image/jpeg", quality);
            document.getElementById("compressedImg").src = compressedDataUrl;
            downloadBtn.href = compressedDataUrl;
            downloadBtn.style.display = "inline-block";
          };
        };
        reader.readAsDataURL(file);
      }, 1000); // 1-second delay for ad click to register
    }
  </script>https://denialtransportationfair.com/p23e981d?key=e35ec5cecd79cd0e6b939867910e619e

